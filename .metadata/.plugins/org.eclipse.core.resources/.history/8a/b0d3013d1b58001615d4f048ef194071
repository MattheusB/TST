
public class Fila {
	public static void main(String[] args) {

	}

	int tamanho;
	int[] fila;
	int inicio;
	int fim;

	public Fila(int tamanho) {
		this.tamanho = tamanho;
		this.fila = new int[tamanho];
		this.inicio = -1;
		this.fim = -1;
	}

	public boolean isEmpty() {
		if (this.inicio == -1 && this.fim == -1) {
			return true;
		}

		return false;
	}

}
